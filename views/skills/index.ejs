<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Skills</title>
</head>
<body>
    <body>
        <h1>My Skills</h1>
        <ul>
          <% skills.forEach(function(s) { %>
            <li>
              <!-- <a href="/todos/:id"></a> -->
            <a href="/skills/<%= s.id %>"><%= s.name %></a>
            </li>
          <% }); %>
        </ul>
      </body>
</body>
</html>